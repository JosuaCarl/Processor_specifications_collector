{
    "extends": "recommended",
    "rules": {
        "CompileStatic": {
            "enabled": false
        },
        "ConsecutiveBlankLines": {
            "enabled": false
        },
        "DuplicateNumberLiteral": {
            "enabled": false
        },
        "DuplicateStringLiteral": {
            "enabled": false
        },
        "Instanceof": {
            "enabled": false
        },
        "JUnitPublicNonTestMethod": {
            "enabled": false
        },
        "MethodParameterTypeRequired": {
            "enabled": false
        },
        "MethodReturnTypeRequired": {
            "enabled": false
        },
        "NoDef": {
            "enabled": false
        },
        "ParameterName": {
            "enabled": false
        },
        "ParameterReassignment": {
            "enabled": false
        },
        "PrintStackTrace": {
            "enabled": false
        },
        "PropertyName": {
            "enabled": false
        },
        "UnnecessaryElseStatement": {
            "enabled": false
        },
        "UnnecessaryGetter": {
            "enabled": false
        },
        "VariableName": {
            "enabled": false
        },
        "VariableTypeRequired": {
            "enabled": false
        }
    }
}